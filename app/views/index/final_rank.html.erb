<div class="container">
  <div class="page-header" id="banner">
    <div class="row">
      <div class="col-lg-8 col-md-7 col-sm-6">
        <h1><strong>最終開票結果</strong></h1>
        <div class="bs-component">
          <table class="table table-striped table-hover">
            <thead>
              <tr>
                <th>順位</th>
                <th>キャラ名</th>
                <th>得票数</th>
              </tr>
            </thead>
            <tbody>
              <% @table.each do |row| %>
                <tr>
                  <td><%= row[0] %></td>
                  <td><%= row[1] %></td>
                  <td><%= row[2] %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div><!-- table part (bs-component) -->
      </div>
    </div>
  </div>
</div>
